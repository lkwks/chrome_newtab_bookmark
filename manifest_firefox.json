{
    "manifest_version": 2,
    "name": "Bookmark",
    "version": "1.0",
    "description": "Displays bookmarks on the new tab page",
    "chrome_url_overrides" : {
        "newtab": "index.html"
    },
    "browser_specific_settings": {
      "gecko": {
          "id": "id@lkwks.pe.kr",
          "strict_min_version": "88.0"
      }
    },    
    "permissions": ["bookmarks", "storage"],
    "options_ui": {
        "page": "options.html",
        "browser_style": true
    }
}
